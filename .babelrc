{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx", 
    "transform-runtime"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["transform-vue-jsx", "istanbul"]
    },
    "utils": {
      "plugins": [
        ["module-resolver", {
          "root": ["oview-ui"],
          "alias": {
            "oview-ui/src": "oview-ui/lib"
          }
        }]
      ]
    }
  }
}
